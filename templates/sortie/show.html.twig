{% extends 'base.html.twig' %}

{% block title %}Sortie{% endblock %}

{% block body %}
    <h1>Sortie</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ sortie.id }}</td>
            </tr>
            <tr>
                <th>Portable</th>
                <td>{{ sortie.portable }}</td>
            </tr>
            <tr>
                <th>Photo</th>
                <td>{{ sortie.photo }}</td>
            </tr>
            <tr>
                <th>OpenTo</th>
                <td>{{ sortie.presentation }}</td>
            </tr>
            <tr>
                <th>Intitule</th>
                <td>{{ sortie.intitule }}</td>
            </tr>
            <tr>
                <th>EntreFille</th>
                <td>{{ sortie.entreFille ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>NbPersonneMax</th>
                <td>{{ sortie.nbPersonneMax }}</td>
            </tr>
            <tr>
                <th>Statut</th>
                <td>{{ sortie.statut }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('sortie_index') }}">back to list</a>

    <a href="{{ path('sortie_edit', {'id': sortie.id}) }}">edit</a>

    {{ include('sortie/_delete_form.html.twig') }}
{% endblock %}
